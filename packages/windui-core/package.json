{
	"name": "@windi-ui/core",
	"version": "0.7.1",
	"license": "MIT",
	"author": "Waleed Khaled <https://github.com/Waleed-KH>",
	"description": "Modern, modular, and customizable UI component library",
	"keywords": ["ui", "component"],
	"repository": {
		"type": "git",
		"url": "https://github.com/windi-ui/windi-ui.git"
	},
	"bugs": {
		"url": "https://github.com/windi-ui/windi-ui/issues"
	},

	"type": "module",
	"scripts": {
		"build": "unbuild",
		"stub": "unbuild --stub"
	},

	"module": "dist/index.mjs",
	"main": "dist/index.cjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		},
		"./components": {
			"import": "./dist/components.mjs",
			"require": "./dist/components.cjs",
			"types": "./dist/components.d.ts"
		}
	},
	"files": ["dist"],

	"dependencies": {
		"csstype": "^3.1.3"
	}
}
