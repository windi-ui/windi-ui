{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"config:base",
		"group:allNonMajor",
		":semanticCommitTypeAll(chore)"
	],
	"rangeStrategy": "bump",
	"automerge": true,
	"automergeStrategy": "squash",
	"packageRules": [{
		"matchDepTypes": ["peerDependencies"],
		"enabled": false
	}]
}

