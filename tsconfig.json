{
	"compilerOptions": {
		"module": "esnext",
		"target": "es2017",
		"lib": ["esnext"],
		"types": ["node"],
		"esModuleInterop": true,
		"strict": true,
		"strictNullChecks": true,
		"moduleResolution": "node",
		"isolatedModules": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"noImplicitAny": false
	},
	"exclude": [
		"**/dist",
		"**/node_modules",
		"**/test",
		"dist"
	]
}