{
	"name": "windui-monorepo",
	"version": "0.0.0",
	"description": "",
	"private": true,
	"scripts": {
		"build": "pnpm -r --filter !./playground/* run build",
		"stub": "pnpm -r --parallel run stub",
		"play": "pnpm -C playground/vue-tailwind run dev",
		"playv4": "pnpm -C playground/vue-tailwindv4 run dev",
		"dev": "pnpm stub && pnpm play",
		"devv4": "pnpm stub && pnpm playv4",
		"lint": "eslint --cache .",
		"lint:fix": "pnpm lint --fix"
	},
	"devDependencies": {
		"@eslint/js": "^9.33.0",
		"@tsconfig/node20": "^20.1.2",
		"@types/node": "24.1.0",
		"eslint": "^9.32.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.39.1",
		"unbuild": "^3.6.0",
		"vite": "^7.0.6"
	}
}
